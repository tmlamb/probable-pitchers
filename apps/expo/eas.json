{
  "cli": {
    "version": ">= 0.55.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "APP_ENV": "development"
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "env": {
        "APP_ENV": "preview",
        "API_BASE_URL": "https://dev.probablepitcher.com",
        "NEXTAUTH_URL": "https://dev.probablepitcher.com"
      }
    },
    "production": {
      "channel": "production",
      "env": {
        "APP_ENV": "production",
        "API_BASE_URL": "https://probablepitchers.net",
        "NEXTAUTH_URL": "https://probablepitchers.net"
      },
      "autoIncrement": true
    }
  }
}
