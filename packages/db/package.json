{
  "name": "@probable/db",
  "version": "0.0.0",
  "main": "./index.ts",
  "types": "./index.ts",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "prisma studio --port 5556",
    "db-push": "prisma db push",
    "prisma:inline": "cp ../../node_modules/.prisma/client/*.js ../../node_modules/@prisma/client && cp ../../node_modules/.prisma/client/schema.prisma ../../node_modules/@prisma/client",
    "prisma:migrate": "prisma migrate deploy && npm run prisma:inline",
    "prisma:migrate:dev": "prisma migrate dev && npm run prisma:inline",
    "prisma:generate": "prisma generate && npm run prisma:inline",
    "postinstall": "npm run prisma:generate"
  },
  "dependencies": {
    "@prisma/client": "^4.2.1"
  },
  "devDependencies": {
    "prisma": "^4.2.1",
    "typescript": "4.8.3"
  }
}
